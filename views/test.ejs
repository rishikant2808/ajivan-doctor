<%var d = new Date(new Date().getTime() + 24 * 60 * 60 * 1000) %>
<%var date =d.getDate()+'/'+(d.getMonth()+1)+'/'+ d.getFullYear()%>
<%=date%>
<% data.doctors.forEach(function(x){ %>
   <h4>
    <% x.booked_info.forEach(function(info){ %>
        <h4>
             <%if(info.date==date){%>
             <em><%=info %></em>
             <% }%>
         </h4>
      <% }) %>
        <!-- <em><%= x %></em> -->
    </h4>
 <% }) %>
 